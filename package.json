{
    "name": "pico8-vscode",
    "displayName": "pico8-vscode",
    "description": "Do all the pico8 things in vscode",
    "version": "0.0.1",
    "publisher": "joho",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:pico8"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "languages": [{
            "id": "pico8",
            "extensions": [
                ".p8"
            ],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
			"language": "pico8",
			"scopeName": "source.p8",
			"path": "./syntaxes/pico8.json"
		}],
        "commands": [{
            "command": "pico8.run",
            "title": "Run a pico8 file"
        }],
        "keybindings": [
            { "key": "cmd+r", "command": "pico8.run" }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}